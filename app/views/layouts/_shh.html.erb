<div>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.6.4/jquery.min.js"></script>
    <script type="text/javascript">
      $(document).ready(function() {
        var keys = [];
        var code = [38, 38, 40, 40, 37, 39, 37, 39, 66, 65, 13];
        var keys1=[];
        var code1 = [72,69,77,65,78]
        $("#shh").hide();
        $("#shhh").hide();
        $(document).keydown(function(keyEvent) {
          keys.push(keyEvent.keyCode);
          keys1.push(keyEvent.keyCode);
          if ( keys.length > code.length ) {
            keys.shift();
          }
          if ( keys1.length > code1.length ) {
            keys1.shift();
          }
          if ( keys.toString() == code.toString() ) {
            $('#shh').slideDown('slow');
          }
          if ( keys1.toString() == code1.toString() ) {
            $(window.open("//www.youtube.com/embed/ZZ5LpwO-An4?autoplay=1"));
          }
        });
      });
    </script>
  </div>

    <div class = "konami" id="shh" >
      <a href = "https://github.com/ralphbean/konami-code-tutorial/blob/master/index.html"><img src="http://i.imgur.com/Ay29K.jpg" height=500 width=500 </img></a>
    </div>
     <div class = "konami" id="shhh" >
      <iframe width="420" height="315" src="//www.youtube.com/embed/ZZ5LpwO-An4" frameborder="0" allowfullscreen></iframe>
    </div>
