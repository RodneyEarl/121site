<h1>Search</h1>

<div class="search">
<%= simple_form_for @subSearch, :url => { :action => "create" } do |f| %>

   <div class="search">
  	<%= f.label "Search by Subject" %>
    <%= f.autocomplete_field :subjectname, autocomplete_subject_name_search_path  %>
  </div>

   <div  class = "search">
  	<%= f.label "Search by Course Name" %>
    <%= f.autocomplete_field :courseName, autocomplete_course_name_search_path  %>
  </div>

   <div  class = "search">
  	<%= f.label "Search by Course Number"%>
    <%= f.text_field :courseNum, :autofocus => true %>

  </div>
 
 <%= f.submit "Search", class: "btn btn-medium btn-success" %>

<% end %>
</div>


