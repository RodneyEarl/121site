<h3 style = "margin: 5px;"> Results </h3>
<div class="row", style = "border-bottom: 3px solid black;">
	<div class = "col-md-2">
		<p style = "font-size: 18px; font-weight: bold;">Student Name
			<%= link_to "Sort", :controller => "search", :action => "name", :topic_id => @result.name, :lab => @result.lab%></p>
	</div>
	<div class = "col-md-1">
		<p style = "font-size: 18px; font-weight: bold;">Number</p>
	</div>
	<div class = "col-md-1">
		<p style = "font-size: 18px; font-weight: bold;">Lab
		<%= link_to "Sort", :controller => "search", :action => "lab", :topic_id => @result.name, :lab => @result.lab%></p>
	</div>
	<div class = "col-md-1">
		<p style = "font-size: 18px; font-weight: bold;">Topic
		<%= link_to "Sort", :controller => "search", :action => "topic", :topic_id => @result.name, :lab => @result.lab%></p>
	</div>
	<div class = "col-md-2">
		<p style = "font-size: 18px; font-weight: bold;">Submitted At
		<%= link_to "Sort", :controller => "search", :action => "time", :topic_id => @result.name, :lab => @result.lab%></p>
	</div>
	<div class = "col-md-2 col-md-offset-2">
		<p style = "font-size: 18px; font-weight: bold;">Current Mark
		<%= link_to "Sort", :controller => "search", :action => "grade", :topic_id => @result.name, :lab => @result.lab%></p>
	</div>
</div>
<% if @questions.any? %>
	<%= render partial: 'shared/markquestions', collection: @questions %>
<% end %>